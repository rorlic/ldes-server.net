@prefix ldes:    <https://w3id.org/ldes#> .
@prefix tree:    <https://w3id.org/tree#>.
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix prov:    <http://www.w3.org/ns/prov#> .
@prefix sh:      <http://www.w3.org/ns/shacl#> .

@prefix lsdn:   <https://ldes-server.net/> .
@prefix fetch:  <https://ldes-server.net/fetch#> .

<connections> a ldes:EventStream ;
	tree:shape [ a sh:NodeShape ] ;
	ldes:timestampPath prov:generatedAtTime ;
	ldes:versionOfPath dcterms:isVersionOf ;
  lsdn:fetching [
    fetch:prefixes
      [fetch:prefix "lc"; fetch:namespace <http://semweb.mmlab.be/ns/linkedconnections#>],
      [fetch:prefix "gtfs"; fetch:namespace <http://vocab.gtfs.org/terms#>],
      [fetch:prefix "wgs84"; fetch:namespace <http://www.w3.org/2003/01/geo/wgs84_pos#>]
    ]
.	
