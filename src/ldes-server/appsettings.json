{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft.Hosting": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Npgsql.Command": "Warning",
      "LdesServer": "Information"
    }
  },
  "ConnectionStrings": {
    "Postgres": ""
  },
  "LdesServer": {
    "Bucketization": {
      "LoopDelay": 5000,
      "MemberBatchSize": 5000
    },
    "Pagination": {
      "LoopDelay": 7000,
      "MemberBatchSize": 5000,
      "DefaultPageSize": 250
    }
  },
  "AllowedHosts": "*"
}
